<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<!--onAction="#createNewBranchClick"-->

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ActionBarController">
   <children>
      <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="23.0" prefWidth="171.0" styleClass="titleBtns" text="Repository">
         <font>
            <Font name="Microsoft YaHei" size="12.0" />
         </font>
      </Label>
      <Button fx:id="switchRepoBtn" mnemonicParsing="false" onAction="#switchRepoClick" prefHeight="29.0" prefWidth="171.0" text="Load/Switch repository" />
      <Button fx:id="initRepoBtn" mnemonicParsing="false" onAction="#initRepoClick" prefHeight="29.0" prefWidth="171.0" text="Init repository" />
      <Button fx:id="cloneBtn" mnemonicParsing="false" onAction="#cloneClick" prefHeight="29.0" prefWidth="171.0" text="Clone" />
      <Button fx:id="loadXMLBtn" mnemonicParsing="false" onAction="#loadXMLClick" prefHeight="29.0" prefWidth="171.0" text="Load XML" />
      <Button fx:id="switchUserBtn" mnemonicParsing="false" onAction="#switchUserClick" prefHeight="29.0" prefWidth="171.0" text="Switch user" />
      <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="23.0" prefWidth="171.0" styleClass="titleBtns" text="Commits">
         <font>
            <Font name="Microsoft YaHei" size="12.0" />
         </font>
      </Label>
      <Button fx:id="commitBtn" mnemonicParsing="false" onAction="#commitClick" prefHeight="29.0" prefWidth="171.0" text="Commit" />
      <Button fx:id="showStatusBtn" mnemonicParsing="false" onAction="#showStatusClick" prefHeight="29.0" prefWidth="171.0" text="WC status" />
      <Button fx:id="commitContentBtn" mnemonicParsing="false" onAction="#commitFilesInfoClick" prefHeight="29.0" prefWidth="171.0" text="Commit content" />
      <Button fx:id="commitsTreeBtn" mnemonicParsing="false" onAction="#commitsTreeClick" prefHeight="29.0" prefWidth="171.0" text="Commits tree" />
      <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="23.0" prefWidth="171.0" styleClass="titleBtns" text="Branches">
         <font>
            <Font name="Microsoft YaHei" size="12.0" />
         </font>
      </Label>
      <Button fx:id="newBranchBtn" mnemonicParsing="false" onAction="#createNewBranchClick" prefHeight="29.0" prefWidth="171.0" text="New branch" />
      <Button fx:id="branchesListBtn" mnemonicParsing="false" onAction="#showBrancheListClick" prefHeight="29.0" prefWidth="171.0" text="Branches list" />
      <Button fx:id="deleteBranchBtn" mnemonicParsing="false" onAction="#deleteBranchClick" prefHeight="29.0" prefWidth="171.0" text="Delete branch" />
      <Button fx:id="checkoutBtn" mnemonicParsing="false" onAction="#checkOutBtnClick" prefHeight="29.0" prefWidth="171.0" text="Checkout " />
      <Button fx:id="resetBranchbtn" mnemonicParsing="false" onAction="#resetBranchClick" prefHeight="29.0" prefWidth="171.0" text="Reset branch" />
      <Button fx:id="mergeBtn" mnemonicParsing="false" onAction="#MergeClick" prefHeight="29.0" prefWidth="171.0" text="Merge" />
      <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="23.0" prefWidth="171.0" styleClass="titleBtns" text="Collaboration">
         <font>
            <Font name="Microsoft YaHei" size="12.0" />
         </font>
      </Label>
      <Button fx:id="fetchBtn" mnemonicParsing="false" onAction="#fetchClick" prefHeight="29.0" prefWidth="171.0" text="Fetch" />
      <Button fx:id="pullBtn" mnemonicParsing="false" onAction="#pullClick" prefHeight="29.0" prefWidth="171.0" text="Pull" />
      <Button fx:id="pushBtn" mnemonicParsing="false" onAction="#pushClick" prefHeight="29.0" prefWidth="171.0" text="Push" />
      <Button fx:id="pushNewBranchBtn" mnemonicParsing="false" onAction="#pushNewBranchBtnClick" prefHeight="29.0" prefWidth="171.0" text="Push new branch to RR" />
      <Label alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="23.0" prefWidth="171.0" styleClass="titleBtns" text="Style">
         <font>
            <Font name="Microsoft YaHei" size="12.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#changeStyleClick" prefHeight="29.0" prefWidth="171.0" text="Change style" />
   </children>
</VBox>
